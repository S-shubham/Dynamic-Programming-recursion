// Fibonacci using DP

#include<iostream>
using namespace std;
  
int fiboi(int n)
{
	int fibo0=0,fibo1=1;
	for(int i=2;i<=n;i++)
	{
		int temp=fibo1+fibo0;
		fibo0=fibo1;
		fibo1=temp;
	}
	return fibo1;
}

int fibod(int n)
{
	static int fibo[11];
	if(n==1)
	{
		fibo[0]=0;
		fibo[1]=1;
		return 1;
	}
	return (fibo[n]=fibod(n-1)+fibo[n-2]);
}
int main()
{
	int n=10;
	cout<<"\nIterative is "<<fiboi(n);
	cout<<"\nDP solution :"<<fibod(n);
}
